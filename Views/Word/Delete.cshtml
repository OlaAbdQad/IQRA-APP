@model iqraProject.DTOs.WordDto



<h3>Do you still want to delete this Word?</h3>
<div style="font-size: 130%;">
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            WordName:
        </dt>
        <dd class="col-sm-10">
            @Model.WordName
        </dd>
        <dt class="col-sm-2">
            Symbol:
        </dt>
        <dd class="col-sm-10">
            @Model.Symbol
        </dd>
        <dt class="col-sm-2">
            Sound:
        </dt>
        <dd class="col-sm-10">
            @Model.Sound
        </dd>
        @* <dt class="col-sm-2">
            CategoryName:
        </dt>
        <dd class="col-sm-10">
            @Model.CategoryName
        </dd> *@
        
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> 
        <a asp-action="Index">Back to List</a>
    </form>
</div>